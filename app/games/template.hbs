<h2>Games</h2>

<ul>
  {{#each model as |game|}}
    <li>
      <ul>
        <h3>{{#link-to "game" game }} Game {{game.id}}{{/link-to}}</h3>
        {{#each game.players as |player|}}
          <li> Player {{player.id}} </li>
          <li> Player's profile is {{player.profile}} </li>
          <li> Current User's profile is {{profile.id}} </li>
          <li> Ember game.Player obj is {{game.players}}</li>
        {{/each}}
        {{#if game.full}}
          <p>THIS GAME IS FULL</p>
        {{else}}
          {{join-team game=game joinGame='joinGame'}}
        {{/if}}
      </ul>
    </li>
  {{/each}}
</ul>
