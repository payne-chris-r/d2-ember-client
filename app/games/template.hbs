<h2>Games</h2>

<ul>
  {{#each model as |game|}}
    <li>
      <ul>
        <h3>{{#link-to "game" game }} Game {{game.id}}{{/link-to}}</h3>
        {{#each game.players as |player|}}
          <li> Player {{player.id}} </li>
          {{!-- <li> Ember game.Player obj is {{game.players.length}}</li> --}}
        {{/each}}
        {{join-team game=game joinGame='joinGame'}}
        {{#if game.full?}}
          <button class='btn'>sdlfjsdlkfj</button>
          <p>{{hasSpace}}</p>
        {{/if}}
      </ul>
    </li>
  {{/each}}
</ul>
