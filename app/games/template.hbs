<h2>Games</h2>

<ul>
  {{#each model as |game|}}
    <li>
      <ul>
        <h3>{{#link-to "game" game }} Game {{game.id}}{{/link-to}}</h3>
        {{#each game.players as |player|}}
          <li> Player {{player.id}} </li>
          <li> Ember Player obj is {{player}}</li>
        {{/each}}
        {{join-team game=game}}
      </ul>
      {{!-- change this to if 'hasSpace' is true --}}
      {{!-- {{#if hasSpace}} --}}
        {{!-- <button class='btn'>Join Team</button> --}}
      {{!-- {{/if}} --}}
    </li>
  {{/each}}
</ul>
