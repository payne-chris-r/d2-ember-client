{{#if profile}}
  <td> {{{profile.id}}} </td>
  <td> {{profile.first_name}} {{profile.last_name}}  </td>
  <td> {{profile.nationality}} </td>
  <td>
    {{#if (eql profile_id profile.id)}}
      {{leave-team game=game leaveGame='leaveGame'}}
    {{else}}
      {{no-rights-button name='Player Confirmed'}}
    {{/if}}
  </td>
{{else}}
  <td> &nbsp; </td>
  <td> &nbsp; </td>
  <td> &nbsp; </td>
  <td> {{join-team game=game joinGame='joinGame'}} </td>
{{/if}}
